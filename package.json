{
  "name": "tslint-sourcegraph",
  "title": "TSLint Integration",
  "description": "Shows Tslint warnings and errors in Sourcegraph",
  "publisher": "jlangston",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "actions": [],
    "menus": {
      "editor/title": [],
      "commandPalette": []
    },
    "configuration": {}
  },
  "version": "0.0.0-DEVELOPMENT",
  "license": "MIT",
  "main": "dist/tslint.js",
  "scripts": {
    "tslint": "tslint -p tsconfig.json './src/**/*.ts'",
    "typecheck": "tsc -p tsconfig.json",
    "test": "jest",
    "build": "parcel build --out-file dist/tslint.js src/tslint.ts",
    "serve": "parcel serve --no-hmr --out-file dist/tslint.js src/tslint.ts",
    "watch:typecheck": "tsc -p tsconfig.json -w",
    "watch:build": "tsc -p tsconfig.dist.json -w",
    "watch:test": "npm run test -- -0watch",
    "sourcegraph:prepublish": "npm run build"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Edge versions",
    "last 1 Safari versions"
  ],
  "devDependencies": {
    "@sourcegraph/tsconfig": "^3.0.0",
    "@sourcegraph/tslint-config": "^12.0.0",
    "@types/jest": "^23.3.8",
    "@types/lz-string": "^1.3.32",
    "lz-string": "^1.4.4",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "parcel-bundler": "^1.10.3",
    "sourcegraph": "^18.1.0",
    "tslint": "^5.11.0",
    "tslint-playground": "git+https://github.com/palantir/tslint-playground.git",
    "typescript": "^3.1.3",
    "ts-jest": "^23.10.4"
  }
}
